<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextBigBold" parent="TextAppearance.AppCompat">
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="TextBigNormal" parent="TextAppearance.AppCompat">
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="TextMediumNormal" parent="TextAppearance.AppCompat">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="TextMediumBold" parent="TextAppearance.AppCompat">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
    </style>
</resources>